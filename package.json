{
  "name": "ts-library-starter",
  "version": "0.0.5",
  "description": "Example git project that is used for typescript libraries as a starter pack",
  "main": "lib/main.js",
  "typings": "lib/main.d.ts",
  "jam": {
    "main": "./lib/main.browser.js"
  },
  "browser": {
    "lib/main.js": "./lib/main.browser.js"
  },
  "scripts": {
    "typings": "typings",
    "prebuild": "npm install && npm run typings install",
    "build": "webpack || echo not ok",
    "build:test": "npm run build && webpack --config webpack.config.test.js",
    "test": "npm run build:test && mocha build/test.js",
    "postinstall": "typings install",
    "pack": "npm run test && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DxCx/ts-library-starter.git"
  },
  "keywords": [
    "example",
    "typescript",
    "library",
    "starter",
    "webpack",
    "browserify",
    "dts-bundle",
    "package",
    "tslint",
    "mocha",
    "chai",
    "babel",
    "tsloader",
    "ts-loader"
  ],
  "author": "Hagai Cohen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DxCx/ts-library-starter/issues"
  },
  "homepage": "https://github.com/DxCx/ts-library-starter#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "delete-empty": "^0.1.3",
    "dts-bundle": "^0.5.0",
    "mocha": "^2.5.3",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-node-externals": "^1.3.3"
  }
}
